<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>AddMinsk</servlet-name>
        <servlet-class>pack.MinskServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddWashington</servlet-name>
        <servlet-class>pack.WashingtonServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBeijing</servlet-name>
        <servlet-class>pack.BeijingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddMinsk</servlet-name>
        <url-pattern>/minsk</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddWashington</servlet-name>
        <url-pattern>/washington</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBeijing</servlet-name>
        <url-pattern>/beijing</url-pattern>
    </servlet-mapping>
</web-app>
